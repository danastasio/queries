{
    "app-id" : "com.github.limads.queries",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "42",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : "org.freedesktop.Sdk.Extension.rust-stable",
    "build-options": { "append-path": "/usr/lib/sdk/rust-stable/bin" },
    "command" : "queries",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland"
        "--filesystem=xdg-documents",
        "--filesystem=home",
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "queries",
            "builddir" : true,
            "buildsystem" : "cargo",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "file:///home/user/Projects/hello"
                }
            ]
        }
    ]
}

